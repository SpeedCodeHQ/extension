{
  "name": "speed-code",
  "displayName": "Speed Code",
  "description": "The VSCode extension for SpeedCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "speed-code.startSpeedRun",
