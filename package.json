{
  "name": "speed-code",
  "displayName": "Speed Code",
  "description": "The VSCode extension for SpeedCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "speed-code.startSpeedRun",
        "title": "Start Speed Run"
      },
      {
        "command": "speed-code.cancelSpeedRun",
        "title": "Cancel Speed Run"
      }
    ],
    "keybindings": [
      {
        "command": "speed-code.submitSpeedRun",
        "key": "shift+enter",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "@types/vscode": "^1.70.0",
    "tslint": "^6.0.0",
    "typescript": "^5.0.0"
  }
}
